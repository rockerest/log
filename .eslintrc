{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "amd": true
    },
    "globals": {
        "requirejs": false
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "accessor-pairs": 2,
        "array-bracket-spacing": [
            2,
            "always"
        ],
        "array-callback-return": 0,
        "arrow-body-style": [
            2,
            "as-needed"
        ],
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": [
            2,
            "always"
        ],
        "brace-style": [
            2,
            "stroustrup"
        ],
        "callback-return": 0,
        "camelcase": 2,
        "comma-spacing": 0,
        "comma-style": [
            2,
            "last"
        ],
        "complexity": [ 1, 3 ],
        "computed-property-spacing": 0,
        "consistent-return": 0,
        "consistent-this": 0,
        "curly": 2,
        "default-case": 0,
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": 0,
        "eol-last": 0,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 2,
        "global-require": 0,
        "guard-for-in": 0,
        "handle-callback-err": 2,
        "id-blacklist": 2,
        "id-length": 0,
        "id-match": 2,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "init-declarations": 0,
        "jsx-quotes": 2,
        "key-spacing": 2,
        "keyword-spacing": 0,
        "linebreak-style": 0,
        "lines-around-comment": 0,
        "max-depth": 2,
        "max-len": 0,
        "max-nested-callbacks": 2,
        "max-params": 0,
        "new-cap": 2,
        "new-parens": 2,
        "newline-after-var": 2,
        "newline-per-chained-call": 0,
        "no-alert": 0,
        "no-array-constructor": 2,
        "no-bitwise": 0,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-confusing-arrow": 2,
        "no-continue": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-function": 0,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 0,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-extra-parens": 0,
        "no-floating-decimal": 2,
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-invalid-this": 0,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 0,
        "no-loop-func": 2,
        "no-magic-numbers": 0,
        "no-mixed-requires": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-spaces": 0,
        "no-multi-str": 2,
        "no-multiple-empty-lines": 2,
        "no-native-reassign": 2,
        "no-negated-condition": 0,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 0,
        "no-path-concat": 2,
        "no-plusplus": 0,
        "no-process-env": 2,
        "no-process-exit": 2,
        "no-proto": 2,
        "no-restricted-imports": 2,
        "no-restricted-modules": 2,
        "no-restricted-syntax": 2,
        "no-return-assign": 0,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 0,
        "no-sync": 0,
        "no-ternary": 0,
        "no-throw-literal": 2,
        "no-trailing-spaces": [
            2,
            {
                "skipBlankLines": true
            }
        ],
        "no-undef-init": 2,
        "no-undefined": 0,
        "no-underscore-dangle": 2,
        "no-unmodified-loop-condition": 2,
        "no-unneeded-ternary": 0,
        "no-unused-expressions": 0,
        "no-use-before-define": 0,
        "no-useless-call": 0,
        "no-useless-concat": 0,
        "no-useless-constructor": 2,
        "no-var": 0,
        "no-void": 2,
        "no-warning-comments": 0,
        "no-whitespace-before-property": 2,
        "no-with": 2,
        "object-curly-spacing": [
            2,
            "always"
        ],
        "object-shorthand": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 2,
        "operator-assignment": [
            2,
            "always"
        ],
        "operator-linebreak": 2,
        "padded-blocks": [
            2,
            "never"
        ],
        "prefer-arrow-callback": 0,
        "prefer-const": 2,
        "prefer-reflect": 0,
        "prefer-rest-params": 0,
        "prefer-spread": 0,
        "prefer-template": 0,
        "quote-props": 2,
        "quotes": [
            2,
            "double",
            "avoid-escape"
        ],
        "radix": [
            2,
            "always"
        ],
        "require-jsdoc": 0,
        "require-yield": 2,
        "semi": 2,
        "semi-spacing": 0,
        "sort-imports": 0,
        "sort-vars": 2,
        "space-before-blocks": [
            2,
            "never"
        ],
        "space-before-function-paren": [
            2,
            "never"
        ],
        "space-in-parens": [
            2,
            "always"
        ],
        "space-infix-ops": 2,
        "space-unary-ops": 0,
        "spaced-comment": [
            2,
            "always"
        ],
        "strict": 0,
        "template-curly-spacing": 2,
        "valid-jsdoc": 2,
        "vars-on-top": 2,
        "wrap-regex": 2,
        "yield-star-spacing": 2,
        "yoda": [
            2,
            "never"
        ]
    }
}
